{
    "name": "api-easy/api-easy-demo",
    "description": "Demo of api-easy framwork",
    "keywords": ["api-easy", "API", "REST", "framwork"],
    "version": "0.1.0",
    "type": "project",
    "require": {
        "php": ">=5.5",
        "ext-SPL": "*",
        "api-easy/api-easy": ">=0.1.11"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/App/Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
